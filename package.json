{
  "name": "bsy-components",
  "version": "0.0.8",
  "description": "新版igroot组件库",
  "repository": "https://github.com/baishancloudFE/bsy-components",
  "license": "MIT",
  "authors": {
    "name": "hanguihe",
    "email": "592029487@qq.com"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "init": "rm -rf node_modules dist es lib && yarn install && yarn run start",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build && node ./scripts/moveDeclare.js",
    "clean": "lerna clean --yes",
    "bootstrap": "lerna bootstrap",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/hooks": "^1.9.2",
    "@umijs/preset-react": "^1.4.20",
    "antd": "^4.2.0",
    "classnames": "^2.2.6",
    "dumi": "^1.0.23",
    "father-build": "^1.18.0",
    "immer": "^6.0.5",
    "lint-staged": "^10.0.7",
    "prettier": "^2.0.5",
    "rc-resize-observer": "^0.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "vfile-message": "^2.0.4",
    "xlsx": "^0.16.0",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "@umijs/test": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^7.0.0",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fs-extra": "^9.0.0",
    "klaw-sync": "^6.0.0"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
