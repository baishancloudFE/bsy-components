{
  "name": "bsy-components",
  "version": "0.0.3",
  "description": "新版igroot组件库",
  "repository": "https://github.com/baishancloudFE/bsy-components",
  "license": "MIT",
  "authors": {
    "name": "hanguihe",
    "email": "592029487@qq.com"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "init": "rm -rf node_modules dist es lib && yarn install && yarn run start",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build && node ./scripts/moveDeclare.js",
    "clean": "lerna clean --yes",
    "bootstrap": "lerna bootstrap",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/hooks": "^1.9.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "antd": "^4.1.0",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "dumi": "^1.0.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "father": "^2.29.2",
    "father-build": "^1.17.2",
    "fs-extra": "^9.0.0",
    "immer": "^6.0.2",
    "klaw-sync": "^6.0.0",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rc-resize-observer": "^0.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-window": "^1.8.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/react-window": "^1.8.1",
    "@types/vfile-message": "^2.0.0"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
